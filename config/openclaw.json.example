{
  // OpenClaw uses JSON5 â€” comments and trailing commas are valid.
  // Verify against https://docs.openclaw.ai/gateway/configuration-reference
  // for your installed version.

  "agents": {
    "defaults": {
      "model": {
        "primary": "google/gemini-3-flash-preview"
      },
      "workspace": "~/.openclaw/workspace",
      "sandbox": {
        "mode": "all",
        "workspaceAccess": "ro",
        "docker": {
          "network": "none"
        }
      }
    }
  },
  "tools": {
    "allow": ["read", "message", "web_search", "web_fetch"],
    "deny": ["exec", "process", "write"]
  },
  "gateway": {
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "REPLACE_WITH_OUTPUT_OF_openssl_rand_-hex_32"
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "REPLACE_WITH_BOTFATHER_TOKEN",
      "groupPolicy": "allowlist",
      "dmPolicy": "pairing",
      "allowFrom": ["tg:REPLACE_WITH_YOUR_TELEGRAM_USER_ID"],
      "streaming": "off",
      "network": {
        "autoSelectFamily": true,
        "dnsResultOrder": "ipv4first"
      }
    }
  }
}
